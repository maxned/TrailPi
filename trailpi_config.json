{
    "site_number": 1,
    "resolution": [3280, 2464],
    "inter_capture_delay": 0.25,
    "quality": 10,
    "capture_chunk_size": 5,

    "camera_type": "day",
    "day_to_night_analog_threshold": 7.5,
    "night_to_day_analog_threshold": 2,
    "day_to_night_digital_threshold": 1.3,
    "night_to_day_digital_threshold": 1.1,
    "ir_led_always_on": true,

    "pir_input_pin": "BOARD11",
    "pir_output_pin": "BOARD36",
    "pi2_input_pin": "BOARD38",
    "pi2_output_pin": "BOARD40",

    "debug": false,
    "image_timestamp_format": "%m:%d:%y-%H:%M:%S:%f",

    "image_folder": "images",
    "image_upload_url": "http://flask-server.wqwtbemyjw.us-west-2.elasticbeanstalk.com/TrailPiServer/api/image_transfer",
    "image_upload_check_interval_sec": 5,
    "image_upload_valid_image_delta": 2,

    "check_in_url": "http://flask-server.wqwtbemyjw.us-west-2.elasticbeanstalk.com/TrailPiServer/api/check_in",
    "check_in_interval_sec": 1800
}
